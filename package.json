{
  "name": "aicrafters.aicademy",
  "version": "1.0.0",
  "description": "AI-powered platform for creating and managing crafts",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd aicrafters-frontend && npm install",
    "install:backend": "cd aicrafters-backend && npm install",
    "start:frontend": "cd aicrafters-frontend && npm start",
    "start:backend": "cd aicrafters-backend && npm run dev",
    "dev": "npx concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azertfx/aicrafters.git"
  },
