  },
  "homepage": "https://github.com/azertfx/aicrafters#readme",
  "dependencies": {
    "@react-spring/web": "^9.7.5",
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "mongodb": "^6.14.0",
    "react-draggable": "^4.4.6",
    "react-markdown": "^10.1.0",
    "react-spring": "^9.7.5"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "concurrently": "^8.2.2"
  }
